"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
const numbers_1 = require("./numbers");
describe("lib -> numbers", () => {
    describe("formatNumber", () => {
        test("a number is converted to a string", () => {
            expect((0, numbers_1.formatNumber)(2)).toBe("2");
        });
        test("a number longer than 4 digits should be formatted", () => {
            expect((0, numbers_1.formatNumber)(2000)).toBe("2,000");
            expect((0, numbers_1.formatNumber)(10000)).toBe("10,000");
            expect((0, numbers_1.formatNumber)(1234567)).toBe("1,234,567");
        });
        test("should handle negative numbers", () => {
            expect((0, numbers_1.formatNumber)(-1234567)).toBe("-1,234,567");
        });
        test("should handle zero", () => {
            expect((0, numbers_1.formatNumber)(0)).toBe("0");
        });
        test("should handle floating point numbers (assuming US locale)", () => {
            expect((0, numbers_1.formatNumber)(12345.6789)).toBe("12,345.679");
        });
    });
});
